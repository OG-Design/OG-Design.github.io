// addNewArithmeticExpression;

document.createElement("h2").innerHTML="1";

// adds EventListener for click inside body
let clickEvent = document.addEventListener("click", newExpression);

let ac_js = document.querySelector("ac_html");

// new expression function
function newExpression() {
    
    let newElementCreate = document.createElement("h2");
    
    
    newElementCreate.id = 0;

    if (ac_js == clickEvent) {
        
        newElementCreate;
        newElementCreate.innerHTML="AC";
        newElementCreate.id ++ ;

        console.log("AC Clicked");
    
    } else {
        console.log("noEffect");
    }
}

newExpression();